<!DOCTYPE html>
<html>
  <head>
    <title>Template</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= stylesheet_link_tag    params[:controller] %>
    <%= javascript_include_tag 'application'%>
  </head>

  <body>
    <%= render 'shared/nav' %>
    <%= render 'shared/sidebar' %>
    <%= yield %>
    <%= render 'shared/footer' %>
    <%= yield 'javascript_content' %>
  </body>
</html>
